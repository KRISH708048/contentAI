generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AIOutput {
  id            Int      @id @default(autoincrement())
  formData      Json
  aiResponse    String?
  templatesSlug String
  createdBy     String
  createdAt     DateTime @default(now())
}
